{
  "name": "peertube-plugin-channel-explorer",
  "description": "Allow users to explore channels with videos in the Discover page",
  "version": "0.1",
  "author": "ja-ha",
  "bugs": "https://github.com/ja-ha/peertube-plugin-channel-explorer/issues",
  "clientScripts": [
    {
      "script": "dist/common-client-plugin.js",
      "scopes": [
        "common"
      ]
    }
  ],
  "css": [
    "assets/style.css"
  ],
  "engine": {
    "peertube": ">=4.2.0"
  },
  "homepage": "https://github.com/ja-ha/peertube-plugin-channel-explorer",
  "keywords": [
    "peertube",
    "plugin",
    "admin",
    "stats"
  ],
  "library": "./main.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "node ./scripts/build.js"
  },
  "staticDirs": {
    "images": "public/images"
  },
  "translations": {
    "fr-FR": "./languages/fr.json",
    "de-DE": "./languages/de.json"
  },
  "devDependencies": {
    "esbuild": "^0.14.36"
  },
  "dependencies": {
    "moment": "^2.29.4"
  }
}
